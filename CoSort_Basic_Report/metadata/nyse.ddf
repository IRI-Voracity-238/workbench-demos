/FILE=NYSE-A
/FIELD=(NewField, TYPE=ASCII, POSITION=1)
/FIELD=(Issue, TYPE=ASCII, POSITION=1, SEPARATOR="\t")
/FIELD=(Symbol, TYPE=ASCII, POSITION=2, SEPARATOR="\t")
/FIELD=(LastTrade, TYPE=NUMERIC, POSITION=3, SIZE=5.2, SEPARATOR="\t", FILL='0')
/FIELD=(Volume, TYPE=NUMERIC, POSITION=4, SIZE=9.0, SEPARATOR="\t", MILL)
/FIELD=(Change, TYPE=NUMERIC, POSITION=5, SIZE=4.2, SEPARATOR="\t", FILL='0')
/FIELD=(Filler, TYPE=ASCII, POSITION=6, SEPARATOR="\t")
/FIELD=(Percent, TYPE=NUMERIC, POSITION=7, SIZE=4.2, SEPARATOR="\t", FILL='0')
