/INFILE=uniqueseedgeneration
    /PROCESS=RANDOM
    /ALIAS=uniqueseedgeneration
    /INCOLLECT=200
    /FIELD=(EMPLOYEE_ID, TYPE=WHOLE_NUMBER, SIZE=12, PRECISION=0, POSITION=1, SEPARATOR="\t")

/SORT
    /KEY=(EMPLOYEE_ID)
    /NODUPLICATES

/OUTFILE=EMPLOYEE_ID.sed
    /PROCESS=RECORD
    /OUTCOLLECT=100
    /FIELD=(EMPLOYEE_ID, POSITION=1, TYPE=ASCII, SEPARATOR="\t")