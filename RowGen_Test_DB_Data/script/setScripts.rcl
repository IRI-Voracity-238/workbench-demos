/INFILE=setgeneration
    /PROCESS=RANDOM
    /INCOLLECT=200
    /FIELD=(DPSID, POSITION=1, SIZE=9, SEPARATOR="\t", TYPE=ALPHA_DIGIT)
    /FIELD=(EE_SEQ, POSITION=2, SIZE=4, PRECISION=0, SEPARATOR="\t", TYPE=NUMERIC)
    /FIELD=(EMPLOYEE_ID, POSITION=3, SIZE=22, PRECISION=0, SEPARATOR="\t", TYPE=NUMERIC)
    /FIELD=(SSN, POSITION=4, SIZE=9, SEPARATOR="\t", TYPE=ALPHA_DIGIT)
    /FIELD=(COMPANY_NUMBER, POSITION=5, SIZE=4, PRECISION=0, SEPARATOR="\t", TYPE=NUMERIC)
    /FIELD=(JOB_CODE, POSITION=6, SIZE=5, SEPARATOR="\t", TYPE=ALPHA_DIGIT)
    /FIELD=(OU_CODE, POSITION=7, SIZE=5, SEPARATOR="\t", TYPE=ALPHA_DIGIT)

/SORT
    /KEY=(DPSID)
    /KEY=(EE_SEQ)
    /KEY=(EMPLOYEE_ID)
    /KEY=(SSN)
    /KEY=(COMPANY_NUMBER)
    /KEY=(JOB_CODE)
    /KEY=(OU_CODE)
    /NODUPLICATES

/OUTFILE=DPSID.set
    /PROCESS=RECORD
    /OUTCOLLECT=100
    /FIELD=(DPSID, POSITION=1, SIZE=9, SEPARATOR="\t", TYPE=ALPHA_DIGIT)

/OUTFILE=EE_SEQ.set
    /PROCESS=RECORD
    /OUTCOLLECT=100
    /FIELD=(EE_SEQ, POSITION=1, SIZE=4, PRECISION=0, SEPARATOR="\t", TYPE=NUMERIC)

/OUTFILE=EMPLOYEE_ID.set
    /PROCESS=RECORD
    /OUTCOLLECT=100
    /FIELD=(EMPLOYEE_ID, POSITION=1, SIZE=22, PRECISION=0, SEPARATOR="\t", TYPE=NUMERIC)

/OUTFILE=SSN.set
    /PROCESS=RECORD
    /OUTCOLLECT=100
    /FIELD=(SSN, POSITION=1, SIZE=9, SEPARATOR="\t", TYPE=ALPHA_DIGIT)

/OUTFILE=COMPANY_NUMBER.set
    /PROCESS=RECORD
    /OUTCOLLECT=100
    /FIELD=(COMPANY_NUMBER, POSITION=1, SIZE=4, PRECISION=0, SEPARATOR="\t", TYPE=NUMERIC)

/OUTFILE=JOB_CODE.set
    /PROCESS=RECORD
    /OUTCOLLECT=100
    /FIELD=(JOB_CODE, POSITION=1, SIZE=5, SEPARATOR="\t", TYPE=ALPHA_DIGIT)

/OUTFILE=OU_CODE.set
    /PROCESS=RECORD
    /OUTCOLLECT=100
    /FIELD=(OU_CODE, POSITION=1, SIZE=5, SEPARATOR="\t", TYPE=ALPHA_DIGIT)