#/LIBRARY="C:\\IRI\\lib\\libcscrypt.dll"

/INFILE="PATIENT_RECORD_ENCRYPTED;DSN=Oracle"
    /PROCESS=odbc
    /ALIAS=patient_record
    /FIELD=(IDNUMBER, ASCII, POSITION=1, SEPARATOR='|')
    /FIELD=(NAME1, ASCII, POSITION=2, SEPARATOR='|')
    /FIELD=(AGE, ASCII, POSITION=3, SEPARATOR='|')
    /FIELD=(EMAIL, ASCII, POSITION=4, SEPARATOR='|')
    /FIELD=(PHONE, ASCII, POSITION=5, SEPARATOR='|')
    /FIELD=(CREDIT_CARD, ASCII, POSITION=6, SEPARATOR='|')
    /FIELD=(DRIVING_LICENSE, ASCII, POSITION=7, SEPARATOR='|')
    /FIELD=(SSN, ASCII, POSITION=8, SEPARATOR='|')
    /FIELD=(CITY, ASCII, POSITION=9, SEPARATOR='|')
    /FIELD=(STATE, ASCII, POSITION=10, SEPARATOR='|')
    
/REPORT

/OUTFILE="PATIENT_RECORD;DSN=Oracle"
	/PROCESS=ODBC
	/CREATE
    /FIELD=(IDNUMBER, ASCII, POSITION=1, SEPARATOR='|')
    /FIELD=(NAME1, ASCII, POSITION=2, SEPARATOR='|',EXT_FIELD="NAME")
    /FIELD=(AGE, ASCII, POSITION=3, SEPARATOR='|')
    /FIELD=(EMAIL, ASCII, POSITION=4, SEPARATOR='|')
    /FIELD=(PHONE, ASCII, POSITION=5, SEPARATOR='|')
    /FIELD=(CREDIT_CARD=dec_fp_aes256_alphanum(CREDIT_CARD,"file:password"), ASCII, POSITION=6, SEPARATOR='|',EXT_FIELD="CREDIT_CARD")
    /FIELD=(DRIVING_LICENSE=dec_fp_aes256_alphanum(DRIVING_LICENSE,"jayavel"), ASCII, POSITION=7, SEPARATOR='|',EXT_FIELD="DRIVING_LICENSE")
    /FIELD=(SSN, ASCII, POSITION=8, SEPARATOR='|')
    /FIELD=(CITY, ASCII, POSITION=9, SEPARATOR='|')
    /FIELD=(STATE, ASCII, POSITION=10, SEPARATOR='|')
    
 /OUTFILE=stdout
    /FIELD=(IDNUMBER, ASCII, POSITION=1, SEPARATOR='|')
    /FIELD=(NAME1, ASCII, POSITION=2, SEPARATOR='|',SET=map_name1.set[NAME1])
    /FIELD=(AGE, ASCII, POSITION=3, SEPARATOR='|')
    /FIELD=(EMAIL, ASCII, POSITION=4, SEPARATOR='|')
    /FIELD=(PHONE, ASCII, POSITION=5, SEPARATOR='|')
    /FIELD=(DEC_FP_CREDIT_CARD=dec_fp_aes256_alphanum(CREDIT_CARD,"file:password"), ASCII, POSITION=6, SEPARATOR='|',EXT_FIELD="CREDIT_CARD")
    /FIELD=(DEC_FP_DRIVING_LICENSE=dec_fp_aes256_alphanum(DRIVING_LICENSE,"jayavel"), ASCII, POSITION=7, SEPARATOR='|',EXT_FIELD="DRIVING_LICENSE")
    /FIELD=(SSN, ASCII, POSITION=8, SEPARATOR='|')
    /FIELD=(CITY, ASCII, POSITION=9, SEPARATOR='|')
    /FIELD=(STATE, ASCII, POSITION=10, SEPARATOR='|')