/INFILE="patient_record;DSN=mysql"
    /PROCESS=ODBC
    /ALIAS=patient_record
    /FIELD=(NUMBER, ASCII, POSITION=1, SEPARATOR='|')
    /FIELD=(NAME, ASCII, POSITION=2, SEPARATOR='|')
    /FIELD=(AGE, ASCII, POSITION=3, SEPARATOR='|')
    /FIELD=(EMAIL, ASCII, POSITION=4, SEPARATOR='|')
    /FIELD=(PHONE, ASCII, POSITION=5, SEPARATOR='|')
    /FIELD=(CREDIT_CARD, ASCII, POSITION=6, SEPARATOR='|')
    /FIELD=(DRIVING_LICENSE, ASCII, POSITION=7, SEPARATOR='|')
    /FIELD=(SSN, ASCII, POSITION=8, SEPARATOR='|')
    /FIELD=(CITY, ASCII, POSITION=9, SEPARATOR='|')
    /FIELD=(STATE, ASCII, POSITION=10, SEPARATOR='|')
    /FIELD=(START_DATE, ISO_DATE, POSITION=11, SEPARATOR='|')
    /FIELD=(END_DATE, ISO_DATE, POSITION=12, SEPARATOR='|')

/SORT 
	/KEY=NAME

/OUTFILE=map_name.set
	/FIELD=(NAME, ASCII, POSITION=1, SEPARATOR='\t')
	/FIELD=(REID_NAME, ASCII, POSITION=2, SEPARATOR='\t', SET=fullnames.set)