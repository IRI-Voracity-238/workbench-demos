CREATE TABLE patient_record_encrypted (
	idnumber VARCHAR(8),
	name1 VARCHAR(50),
	age VARCHAR(4),
	email VARCHAR(50),
	phone VARCHAR(30),
	credit_card VARCHAR(20),
	driving_license VARCHAR(30),
	ssn VARCHAR(11),
	city VARCHAR(20),
	state VARCHAR(20)
);

CREATE TABLE patient_record (
	idnumber VARCHAR(8),
	name VARCHAR(50),
	age VARCHAR(4),
	email VARCHAR(50),
	phone VARCHAR(30),
	credit_card VARCHAR(20),
	driving_license VARCHAR(30),
	ssn VARCHAR(11),
	city VARCHAR(20),
	state VARCHAR(20)
);
